{
  "name": "ai-client",
  "version": "0.0.1",
  "description": "AI client",
  "productName": "SKGadi's AI Client",
  "author": "Suresh Kumar Gadi",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "remove-docs": "rm -rf ../docs",
    "copy-docs": "cp -r dist/spa ../docs",
    "build-all": "npm run build && npm run remove-docs && npm run copy-docs",
    "postinstall": "quasar prepare",
    "temp": "quasar new store socket"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@types/marked": "^6.0.0",
    "marked": "^15.0.6",
    "pinia": "^2.3.1",
    "quasar": "^2.16.0",
    "socket.io-client": "^4.8.1",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.0.0",
    "@types/node": "^20.5.9",
    "@types/socket.io-client": "^3.0.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.8.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
